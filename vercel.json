{
  "functions": {
    "api/index.py": {
      "includeFiles": "../main.py,../templates/**,../data/**"
    }
  },
  "builds": [
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
